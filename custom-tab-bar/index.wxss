.tab-bar {
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  height: 48px;
  background: #ffffff;
  display: flex;
  /*padding-bottom: env(safe-area-inset-bottom);*/
  z-index: 100
}
.menu_left{
  width: 43%;
  position: fixed;
  bottom: 0;
  left: 0;
  height: 48px;
  background: #ffffff;
  display: flex;
  /*padding-bottom: env(safe-area-inset-bottom);*/
  z-index: 100
}

.menu_right{
  width: 40%;
  position: fixed;
  bottom: 0;
  right: 0;
  height: 48px;
  background: #ffffff;
  display: flex;
  /*padding-bottom: env(safe-area-inset-bottom);*/
  z-index: 100
}

.tab-bar::after{
  width: 58px;
  height: 55px;
  content: "";
  left: 44%;
  top: -40%;
  position: absolute;
  display: block;
  background: #f2f1f9;
  border-radius: 50%;
}
